pipeline {
    agent any
    stages {


        stage('Terraform-init-plan'){
            steps {
                sh """
                echo "executing deployment of subsystem"
                cd ${SUBSYSTEM}
                chmod +x ./deploy.sh
                ./deploy.sh
               """ 
            }
        }

        stage('Cleanup'){
            steps {

                cleanWs()
            }
        }

    }
}
