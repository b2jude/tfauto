pipeline {
    agent any

    stages {
        stage('Terraform init') {
            steps {
                echo 'initializing..'
                terraform init
            }
        }
        stage(' Plan') {
            steps {
                echo 'Planning..'
                terraform plan
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
