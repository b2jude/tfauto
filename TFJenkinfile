pipeline { 
    agent any 
    stages {
        stage('WS Clean') { 
            steps { 
                cleanWs() 
            }
        }
        stage('Terraform init'){
            steps {
                sh 'terraform init'
                sh 'terraform plan'
            }
        }
        stage('WS clean-end') {
            steps {
                cleanWs()
            }
        }
    }
}
