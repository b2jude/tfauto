pipeline {
    agent any
    stages {


        stage('Terraform-init-plan'){
            steps {
                echo " cd to subsystem"
                sh "cd $WORKSPACE/${SUBSYSTEM}"
                echo "cd to subsystem successful"
                sh "./deploy.sh ${COMMAND}"

            }
        }

        stage('Cleanup'){
            steps {

                cleanWs()
            }
        }

    }
}
