pipeline {
    agent any
    stages {


        stage('Terraform-init-plan'){
            steps {
                echo " executing deployment of subsystem"
                sh  chmod + x "$WORKSPACE/${SUBSYSTEM}/deploy.sh
                sh "$WORKSPACE/${SUBSYSTEM}/deploy.sh ${COMMAND}"
                
            }
        }

        stage('Cleanup'){
            steps {

                cleanWs()
            }
        }

    }
}
