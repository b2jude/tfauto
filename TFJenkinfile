pipeline { 
    agent any 
    stages {
        stage('WS Clean') { 
            steps { 
                cleanWs() 
            }
        }
        stage('Terraform-init-plan'){
            steps {
                sh 'deploy.sh'
            }
        }
        stage('WS clean-end') {
            steps {
                cleanWs()
            }
        }
    }
}
