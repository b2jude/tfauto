pipeline { 
    agent any 
    stages {
        stage('WS Clean') { 
            steps { 
                cleanWs() 
            }
        }
        stage('Terraform-init-plan'){
            steps {
                sh '''echo \'executing terraform...\'
                      python --version
                      terraform init
                      echo \'executing pass init\'
                      

                      terraform plan'''

            }
        }
        stage('WS clean-end') {
            steps {
                cleanWs()
            }
        }
    }
}
